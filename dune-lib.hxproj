  <!-- Build options -->
  <build>
    <option directives="openfl=2.2.4&#xA;lime=2.0.6&#xA;actuate=1.8.1&#xA;heaps=0.0.1&#xA;format=3.1.2&#xA;tools=2.0.6&#xA;no-compilation&#xA;openfl-flash&#xA;web" />

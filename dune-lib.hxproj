  </output>
  <!-- Other classes to be compiled into your SWF -->
  <classpaths>
    <class path="C:\HaxeToolkit\haxe\lib\openfl\2,2,4" />
    <class path="C:\HaxeToolkit\haxe\lib\lime\2,0,6" />
    <class path="C:\HaxeToolkit\haxe\lib\actuate\1,8,1" />
    <class path="C:\HaxeToolkit\haxe\lib\heaps\git" />
    <class path="C:\HaxeToolkit\haxe\lib\format\3,1,2" />
    <class path="src" />
    <class path="bin\flash\haxe" />
